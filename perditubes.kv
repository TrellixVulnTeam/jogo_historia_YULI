<Mensagem>:
    BoxLayout:
        id: men
        orientation:'vertical'
        size_hint_y:None
        height:self.minimum_height

<Conteudo>:
    size_hint_y:None
    height:150

    Button:
        border: 0, 0, 0, 0
        background_normal: 'img/Base2.png'
        size_hint_y:.9
        size_hint_x:.2
        font_size: 30

    Button:
        background_color: 1, 1, 1, 1
        background_normal: 'img/fundo_balao.png'
        border: 0, 0, 0, 0
        background_down: 'img/fundo_balao.png'
        id: lab
        font_size: 35
        disabled: False
