#:kivy 1.11.1
<TestTexto>:
    orientation:'vertical'
    height:150
    BoxLayout:
        size_hint:(None, None)
        # orientation:'vertical'
        # Height não tem efeito por conta do .1 do size_hint(None, .1)
        # height:800
        id:box
        width:'45px'
        Button:
            orientation:'vertical'
            border: 0, 0, 0, 0
            background_normal: 'cayla_rosa.png'
            size_hint_y:1
            size_hint_x:None
            font_size: 30
        Button:
            orientation:'vertical'
            # balao_rosa.png
            background_normal: 'balao_rosa.png'
            background_down:'balao_rosa.png'
            id:bot
            text:'SSSSS222222222222222'
            underline: True
            text_size:(None, None)
            halign:'left'
            font_size:'35px'
    BoxLayout:
        size_hint:(None, None)
        # orientation:'vertical'
        # Height não tem efeito por conta do .1 do size_hint(None, .1)
        # height:800
        id:box1
        width:'45px'
        Button:
            orientation:'vertical'
            # balao_rosa.png
            background_normal: 'balao_rosa.png'
            background_down:'balao_rosa.png'
            id:bot1
            underline: True
            text:'!!!'
            text_size:(None, None)
            halign:'left'
            font_size:'35px'
